{#
/**
 * @file
 * Template for a Page Navigation component.
 *
#}


{%
  embed 'artisan_caminofrances:navbar' with {
  container: 'fixed',
  navbar_theme: 'light',
  navbar_utility_classes: [
    'justify-content-between',
  ],
}
%}




  {% block branding %}
    <a class="d-inline-flex align-items-center" data-component-id="artisan:navbar-brand" href="/">
      <div class="navbar-brand__logo">
          <picture>
            {# Logo normal (por defecto) #}
            <img src="/themes/custom/artisan_caminofrances/logo.png" width="256px" height="89px" alt="Barbanza logo" class="navbar-logo-default">
            {# Logo blanco para home (oculto por defecto, se mostrar√° solo en home con CSS) #}
            <img src="/themes/custom/artisan_caminofrances/build/assets/images/logo-barbanza-arousa-blanco.png" width="256px" height="89px" alt="Barbanza logo" class="navbar-logo-white" style="display: none;">
        </picture>
      </div>
    </a>

    {% if page.branding %}
      {{ page.branding }}
    {% endif %}
  {% endblock %}

  {% block left %}
    {% if page.primary_menu %}
      {{ page.primary_menu }}
    {% endif %}
  {% endblock %}

  {% block right %}
    {% if page.secondary_menu %}
      {{ page.secondary_menu }}
    {% endif %}
  {% endblock %}

  {% block header %}
    {% if page.header %}
      {{ page.header }}


    {% endif %}
  {% endblock %}
{% endembed %}
