{#
/**
 * @file
 * Template for a Page Navigation component.
 *
#}


{%
  embed 'artisan_caminofrances:navbar' with {
  container: 'fixed',
  navbar_theme: 'light',
  navbar_utility_classes: [
    'justify-content-between',
  ],
}
%}




  {% block branding %}
    <a class="d-inline-flex align-items-center" data-component-id="artisan:navbar-brand" href="/">
      <div class="navbar-brand__logo">
        {# Logo de color para páginas internas (por defecto) #}
        <img src="/themes/custom/artisan_caminofrances/build/assets/images/logo-barbanza-color.png" width="190px" height="46px" alt="Barbanza logo" class="navbar-logo-color">
        {# Logo blanco para home (oculto por defecto, se mostrará solo en home con CSS) #}
        <img src="/themes/custom/artisan_caminofrances/build/assets/images/logo-barbanza-arousa-blanco.png" width="190px" height="46px" alt="Barbanza logo" class="navbar-logo-white">
      </div>
    </a>

    {% if page.branding %}
      {{ page.branding }}
    {% endif %}
  {% endblock %}

  {% block left %}
    {% if page.primary_menu %}
      {{ page.primary_menu }}
    {% endif %}
  {% endblock %}

  {% block right %}
    {% if page.secondary_menu %}
      {{ page.secondary_menu }}
    {% endif %}
  {% endblock %}

  {% block header %}
    {% if page.header %}
      {{ page.header }}


    {% endif %}
  {% endblock %}
{% endembed %}
