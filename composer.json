{
    "name": "metadrop/aljibe",
    "description": "Project template for Drupal 10",
    "version": "1.0.0",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Metadrop/backstopjs-addons.git"
        }
    ],
    "replace": {
        "sebastian/phpcpd": "*"
    },
    "require": {
        "php": "^8.2.25",
        "chillerlan/php-qrcode": "^5.0",
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^2.0",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.4",
        "drupal/artisan": "^1.0",
        "drupal/block_visibility_conditions": "^2.1",
        "drupal/config_ignore": "^3.3",
        "drupal/config_split": "^2.0",
        "drupal/cookies": "^1.2",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/cshs": "^4.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/entity_access_by_role_field": "^2.0",
        "drupal/environment_indicator": "^4.0",
        "drupal/feeds": "*",
        "drupal/geolocation": "^3.13",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_toolbar": "^1.0@RC",
        "drupal/google_tag": "^2.0",
        "drupal/honeypot": "^2.0",
        "drupal/ludwig": "*",
        "drupal/mail_login": "^4.0",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/menu_item_extras": "^3.1",
        "drupal/metatag": "^2.0",
        "drupal/openweather": "^1.2",
        "drupal/pathauto": "^1.8",
        "drupal/rabbit_hole": "^1.0",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.8",
        "drupal/reroute_email": "^2.2",
        "drupal/roleassign": "^2.0",
        "drupal/schema_metatag": "^3.0",
        "drupal/simple_sitemap": "^4.2",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/taxonomy_menu": "^3.6",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/vlsuite": "2.x-dev@dev",
        "drupal/webform": "^6.2",
        "drupal/webp": "^1.0@RC",
        "drush/drush": "*",
        "metadrop/backstopjs-addons": "^0.0.5",
        "metadrop/drupal-artifact-builder": "^2.0"
    },
    "require-dev": {
        "metadrop/drupal-dev": "^2.0.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "phpro/grumphp": true,
            "metadrop/drupal-boilerplate-assistant": true,
            "metadrop/composer-comments": true,
            "php-http/discovery": true,
            "mxr576/ddqg-composer-audit": true,
            "tbachert/spi": true,
            "cweagans/composer-patches": true
        }
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "mkdir -p tests/backstopjs/common/libraries",
            "cp -R -f vendor/metadrop/backstopjs-addons/ tests/backstopjs/common/libraries/backstopjs-addons/"
        ],
        "post-update-cmd": [
            "mkdir -p tests/backstopjs/common/libraries",
            "cp -R -f vendor/metadrop/backstopjs-addons/ tests/backstopjs/common/libraries/backstopjs-addons/"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "Add a #disable_inline_form_errors_summary property to disable the Inline Form Errors summary - https://drupal.org/i/2880011": "https://www.drupal.org/files/issues/2023-08-22/2880011-107_0.patch",
                "WSOD when x-drupal-cache-tags is too large": "patches/finish-response-subscriber-need-warning-error-when-headers-exceed-16k.patch",
                "[PP-1] bundleFieldDefinitions() are not added in EntityViewsData - https://drupal.org/i/2898635": "patches/drupal-2898635-views-data-bundle-field-definitions-MR-4953-3.patch",
                "Add Views EntityReference filter to support better UX for exposed filters": "https://www.drupal.org/files/issues/2024-08-19/2429699-546-10.3.x.patch"
            },
            "drupal/default_content": {
                "Add a Normalizer and Denormalizer to support Layout Builder - https://www.drupal.org/project/default_content/issues/3160146": "patches/default_content-3160146-layout-builder-support-MR-15-2.patch",
                "Do not reimport existing entities - https://drupal.org/i/2698425": "https://www.drupal.org/files/issues/2024-08-30/2698425-do-not-re-import-existing-entities--acquia-starter-kit--2-0-0alpha3-2024-08-30.patch"
            },
            "drupal/section_library": {
                "3394377 - Preview section in library (main theme like layout builder content design tab)": "patches/3394377-section-library-section-preview-19-1.patch",
                "3263496 - Canâ€™t add existing section to library - https://www.drupal.org/project/section_library/issues/3263496": "patches/3263496-section-library-MR-9-1.patch",
                "When cloning the blocks it does not assign the correct language to the block - https://www.drupal.org/i/3303145": "patches/3303145-section-library-MR-14-1.patch"
            },
            "drupal/entity_access_by_role_field": {
                "Add permission to control access to role field into edit operation - https://drupal.org/i/3419873": "patches/3419873-entity-access-by-role-field-edit-field-permission-MR-25-1.patch"
            },
            "drupal/addtoany": {
                "Attach library to AddToAny render arrays, and add option to only load AddToAny assets upon module use": "https://www.drupal.org/files/issues/2023-07-25/3056377-addtoany-library-attachment-33.patch"
            },
            "drupal/vlsuite": {
                "Remove jQuery dependency from VLSuite Slider": "patches/vlsuite-slider-remove-jquery-dependency.patch"
            }
        }
    }
}
